{
  "expo": {
    "name": "Legatura",
    "slug": "legatura-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "icon": "./assets/images/icons/app-icon.png",
    "platforms": [
      "android",
      "web"
    ],
    "userInterfaceStyle": "light",
