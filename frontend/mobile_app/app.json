    "android": {
      "package": "com.legatura.mobile",
      "icon": "./assets/images/icons/app-icon.png"
    },
    "ios": {
      "icon": "./assets/images/icons/app-icon.png"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "ae2aaa48-b348-43da-b3b1-ec8debee58f1"
      }
    },
    "plugins": [
      "expo-font",
      "@react-native-community/datetimepicker"
    ]
  }
}
